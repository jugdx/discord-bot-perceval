{
  "name": "discord-bot-perceval",
  "version": "1.0.0",
  "description": "A dicord bot with a lot of features (music / gaming / user management / ...)",
  "main": "index.js",
  "scripts": {
    "start-dev-debug": "ts-node-dev --debug --respawn --transpile-only --poll -r tsconfig-paths/register ./src/index.ts",
    "start-dev": "ts-node-dev --respawn --transpile-only --poll -r tsconfig-paths/register ./src/index.ts",
    "start": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "clean": "rimraf dist",
    "clean-build": "rimraf dist && tsc",
    "build": "tsc",
    "build-trace": "tsc --traceResolution",
    "watch:build": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jugdx/discord-bot-perceval.git"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "discord.js"
  ],
  "author": {
    "name": "JuGdx",
    "email": "jugdx@gmail.com",
    "url": "https://github.com/jugdx"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jugdx/discord-bot-perceval/issues"
  },
  "homepage": "https://github.com/jugdx/discord-bot-perceval#readme",
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/node": "^14.14.27",
    "@types/ws": "^7.4.0",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "common-tags": "^1.8.0",
    "consola": "^2.15.3",
    "diskord-bot": "^1.0.0",
    "dotenv": "^8.2.0",
    "ts-node": "^9.1.1"
  }
}
